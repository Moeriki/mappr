{
  "name": "mappr",
  "version": "3.0.0",
  "description": "JavaScript pojo to pojo mapper",
  "repository": "git@github.com:Moeriki/mappr.git",
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js lib/ test/",
    "pretest": "npm run lint",
    "test": "ava",
    "test:coverage": "nyc ava",
    "test:coverage:html": "nyc ava report --reporter=html",
    "build": "BABEL_ENV=production babel lib/ --out-dir dist/",
    "preversion": "npm run build && npm run test:coverage && git add -A dist/"
  },
  "keywords": [
    "mapper",
    "object",
    "pojo"
  ],
  "author": "Dieter Luypaert",
  "license": "MIT",
  "dependencies": {
    "lodash.flow": "^3.4.0",
    "lodash.get": "^4.4.1",
    "lodash.isplainobject": "^4.0.5",
    "lodash.mapvalues": "^4.5.1",
    "lodash.omitby": "^4.6.0",
    "lodash.topairs": "^4.3.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "eslint": "^3.2.2",
    "eslint-config-muriki": "^3.3.3",
    "eslint-plugin-import": "^1.12.0",
    "lodash": "^4.14.1",
    "nyc": "^7.1.0"
  },
  "ava": {
    "require": [
      "babel-core/register"
    ],
    "verbose": true
  },
  "nyc": {
    "exclude": [
      "dist",
      "test"
    ]
  }
}
